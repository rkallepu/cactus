<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/bootstrap/dist/css/bootstrap.css"/>
  <link rel="stylesheet" href="/angular-material/angular-material.css"/>
  <link rel='stylesheet' href='/stylesheets/style.css'/>
  <script type="text/javascript" src="jquery/dist/jquery.js"></script>
  <script type="text/javascript" src="bootstrap/dist/js/bootstrap.js"></script>
  <script type="text/javascript" src="/angular/angular.js"></script>
  <script type="text/javascript" src="/angular-bootstrap/ui-bootstrap.js"></script>
  <script type="text/javascript" src="/angular-bootstrap/ui-bootstrap-tpls.js"></script>
  <script type="text/javascript" src="/angular-ui-router/release/angular-ui-router.js"></script>
  <script type="text/javascript" src="/angular-material/angular-material.js"></script>
  <script type="text/javascript" src="/angular-aria/angular-aria.js"></script>
  <script type="text/javascript" src="/angular-animate/angular-animate.js"></script>
  <script type="text/javascript" src="/angular-smart-table/dist/smart-table.js"></script>
  <script type="text/javascript" src="/angular-ui-utils/event.js"></script>
  <script type="text/javascript" src="/angular-ui-map/ui-map.js"></script>
  <script type="text/javascript" defer src="https://maps.googleapis.com/maps/api/js?v=3.exp&callback=onGoogleReady"></script>

  <script src="/app/app.js"></script>
  <script src="app/mainIndex.js"></script>
  <script src="app/accounts/account.js"></script>
  <script src="app/home/home.js"></script>
  <script src="/app/home/filters.js"></script>

</head>
<body ng-controller="mainController">
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">Welcome to Ride Share</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <li ng-hide="show.Register"><a class="btn btn-default rideshare-style" href="#/register" ui-sref="register">New User</a></li>
          <li ng-hide="show.Login"><a class="btn btn-default rideshare-style" href="#/login" ui-sref="login">Log In</a></li>
          <li ng-hide="show.Logout"><a class="btn btn-default rideshare-style" href="#/" ng-click="logout()">Log Out</a></li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12">
        <div ui-view="viewA"></div>
      </div>
    </div>
  </div>
</body>
</html>
